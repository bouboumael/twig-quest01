{% extends 'layout.html.twig' %}

{% block content %}

<div class="startProducts">
    <p>
        {% for key, product in products %}
            {% if key < 3 %}
                {{ product }}
            {% endif %}
        {% endfor %}
    </p>
</div>
<div class="finishProducts">
    <p>
        {% for key, product in products %}
            {% if key > 2 %}
                {{ product }}
            {% endif %}
        {% endfor %}
    </p>
</div>

    {{ dump(products) }}

{% endblock %}

